{

	"default": "memory",

	"stores": {

		"memory": {
			"driver": "memory",
			"max": 10000,
			"ttl": 86400
		},

		"redis": {
			"driver": "redis",
			"connection": {
				"host": "localhost",
				"port": 6379,
			},
